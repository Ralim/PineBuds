{
	"name": "Open PineBuds Pro - VS Code Development Environment",
	"image": "ghcr.io/pine64/openpinebuds:latest-sdk",
	"build": {
		"build": "Dockerfile"
	},
	"extensions": [
		"ms-vscode.cmake-tools",
		"ms-vscode.cpptools"
	],
	"runArgs": [
		"--privileged"
	],
	"userEnvProbe": "loginInteractiveShell",
	"remoteUser": "root"
}
